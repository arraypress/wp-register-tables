{
  "name": "arraypress/wp-register-tables",
  "version": "1.0.0",
  "description": "A comprehensive WordPress library for creating and managing custom admin tables with advanced filtering, sorting, and interactive features",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-register-tables/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "arraypress/wp-composer-assets": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\CustomTables\\": "src/"
    },
    "files": [
      "src/Utilities.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-register-tables/issues",
    "source": "https://github.com/arraypress/wp-register-tables"
  },
  "keywords": [
    "wordpress",
    "admin",
    "tables",
    "list-tables",
    "wp-list-table",
    "admin-tables",
    "data-tables",
    "custom-tables",
    "filtering",
    "sorting",
    "pagination",
    "wp-admin",
    "interactive",
    "ajax",
    "responsive",
    "management",
    "crud"
  ]
}