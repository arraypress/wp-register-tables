{
  "name": "arraypress/wp-register-submenu-pages",
  "version": "1.0.0",
  "description": "A flexible WordPress submenu pages manager with support for separators and hierarchical organization",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-register-submenu-pages/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\WP\\Register\\": "src/"
    },
    "files": [
      "src/Utilities.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-register-submenu-pages/issues",
    "source": "https://github.com/arraypress/wp-register-submenu-pages"
  },
  "keywords": [
    "wordpress",
    "admin",
    "submenu",
    "pages",
    "menu",
    "admin-menu",
    "wp-admin",
    "separators",
    "navigation",
    "menu-pages",
    "admin-pages",
    "submenus",
    "menu-items",
    "management"
  ]
}